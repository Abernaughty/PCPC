{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,ico}", "/*.css", "/*.js"]
  },
  "globalHeaders": {
    "Cache-Control": "max-age=3600"
  },
  "env": {
    "production": {
      "API_URL": "https://pcpc-apim-prod.azure-api.net/pcpc-api/v1",
      "DEBUG": "false"
    },
    "staging": {
      "API_URL": "https://pcpc-apim-staging.azure-api.net/pcpc-api/v1",
      "DEBUG": "true"
    }
  }
}
