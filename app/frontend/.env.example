# PCPC Frontend Environment Configuration Template
# Copy this file to .env and update with your actual values

# Build Environment
NODE_ENV=development

# API Management Configuration
APIM_BASE_URL=https://your-apim-instance.azure-api.net/pokedata-api
APIM_SUBSCRIPTION_KEY=your-apim-subscription-key-here

# Azure Functions Configuration (fallback)
AZURE_FUNCTIONS_BASE_URL=https://your-function-app.azurewebsites.net/api
AZURE_FUNCTION_KEY=your-function-key-here

# Feature Flags
USE_API_MANAGEMENT=true
DEBUG_API=false

# Development Server
PORT=3000

# Legacy API Configuration (for backward compatibility)
API_BASE_URL=https://your-apim-instance.azure-api.net/pokedata-api/v0

# Application Insights Configuration
VITE_APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=your-instrumentation-key;IngestionEndpoint=https://your-region.in.applicationinsights.azure.com/
VITE_APPLICATIONINSIGHTS_ROLE_NAME=pcpc-frontend
VITE_APP_VERSION=0.2.0
VITE_ENVIRONMENT=development
