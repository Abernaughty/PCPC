{
  "name": "pcpc-frontend",
  "version": "0.2.0",
  "description": "Pokemon Card Price Checker (PCPC) - Svelte Frontend Application",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "cross-env PORT=3000 rollup -c -w",
    "start": "sirv public --no-clear --port 3000",
    "clean": "rimraf node_modules package-lock.json",
    "deploy:frontend": "node scripts/deploy-frontend.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-replace": "6.0.2",
    "cross-env": "10.1.0",
    "dotenv": "16.5.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.2",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-svelte": "7.2.2",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "4.2.19"
  },
  "dependencies": {
    "axios": "1.12.2",
    "js-yaml": "4.1.0",
    "node-fetch": "3.3.2",
    "sirv": "3.0.1",
    "sirv-cli": "1.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "npm@latest",
  "keywords": [
    "svelte",
    "pokemon",
    "card-prices",
    "tcg"
  ]
}
